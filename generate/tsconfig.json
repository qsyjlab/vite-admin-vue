{
  "compilerOptions": {
    "baseUrl": "./",
    "types": ["node"],
    "target": "esnext",
    "noEmit": true,
    "useDefineForClassFields": true,
    "noImplicitAny": false,
    "allowImportingTsExtensions": true,
    // "noUnusedParameters":false,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"]
  }
}
